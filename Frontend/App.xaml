<Application x:Class="Frontend.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Frontend"
             xmlns:viewModel="MainWindowViewModel"
             xmlns:viewModel1="clr-namespace:Frontend.MVVM.ViewModel"
             xmlns:view="clr-namespace:Frontend.MVVM.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <ResourceDictionary Source="Theme/MenuButtonTheme.xaml"></ResourceDictionary>
                 <ResourceDictionary Source="Theme/SearchBarTheme.xaml"></ResourceDictionary>
             </ResourceDictionary.MergedDictionaries>
             
             <DataTemplate DataType="{x:Type viewModel1:HomeViewModel}">
                 <view:HomeView/>
             </DataTemplate>
         </ResourceDictionary>
    </Application.Resources>
</Application>
